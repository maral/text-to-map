{
  "type": "module",
  "dependencies": {
    "@arodax/jtsk2wgs84": "^1.0.2",
    "@turf/distance": "^6.5.0",
    "adm-zip": "^0.5.9",
    "better-sqlite3": "^8.0.1",
    "chevrotain": "^10.4.2",
    "csv-parse": "^5.3.3",
    "czech-address": "^1.0.1",
    "feedparser": "^2.2.10",
    "iconv-lite": "^0.6.3",
    "node-fetch": "^3.3.0",
    "parsedbf": "^1.1.1",
    "sax": "^1.2.4",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@chevrotain/types": "^10.5.0",
    "@jest/globals": "^29.3.1",
    "@types/adm-zip": "^0.5.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/feedparser": "^2.2.5",
    "@types/node": "^18.11.17",
    "copyfiles": "^2.4.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "address-points": "ts-node --experimental-specifier-resolution=node --esm src/bin/address-points.ts",
    "address-points-import-only": "ts-node --experimental-specifier-resolution=node --esm src/bin/address-points-import-only.ts",
    "schools": "ts-node --experimental-specifier-resolution=node --esm src/bin/schools.ts",
    "clear-all": "ts-node --experimental-specifier-resolution=node --esm src/bin/clear-all.ts",
    "parse-ordinance": "ts-node --experimental-specifier-resolution=node --esm src/bin/parse-ordinance.ts",
    "diagrams": "ts-node --experimental-specifier-resolution=node --esm src/bin/generate-syntax-diagrams.ts",
    "streets": "ts-node --experimental-specifier-resolution=node --esm src/bin/streets.ts",
    "regions": "ts-node --experimental-specifier-resolution=node --esm src/bin/regions.ts",
    "dbf": "ts-node --experimental-specifier-resolution=node --esm src/bin/dbf.ts",
    "test": "jest",
    "ordinance-to-csv-rules": "node src/smd-to-csv/ordinance-to-csv-rules.js",
    "clean": "rimraf dist",
    "copyfiles": "copyfiles -u 1 src/address_points_init.db dist",
    "build": "npm-run-all clean -p copyfiles build:*",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:js": "tsc --outDir dist",
    "prepare": "npm run build"
  },
  "name": "text-to-map",
  "description": "Text To Map usiluje o lepší, strojově zpracovatelné využití částí vyhlášek s výčtem ulic a dalších lokací. Jde o rozšiřitelnou sadu konceptů a nástrojů, které zajistí hladký převod výčtu ulic a jejich rozsahů v lidsky srozumitelném jazyce do strojově zpracovatelného, uchopitelného formátu.",
  "version": "1.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maral/text-to-map.git"
  },
  "keywords": [
    "streets",
    "catchment areas",
    "schools",
    "map",
    "geodata",
    "address points",
    "polygons"
  ],
  "author": "Marek Lisý",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maral/text-to-map/issues"
  },
  "homepage": "https://github.com/maral/text-to-map#readme"
}
